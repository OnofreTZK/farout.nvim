    # TokyoNight Color Palette
    set -l foreground E0CCAE
    set -l selection 6b442f
    set -l comment 6B4035
    set -l red bf472c
    set -l orange ff9e64
    set -l yellow f2a766
    set -l green a4896f
    set -l purple a47a7a
    set -l cyan a67458
    set -l pink 8a4b53

    # Syntax Highlighting Colors
    set -g fish_color_normal $foreground
